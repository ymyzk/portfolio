{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Portfolio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymyzk/portfolio.git"
  },
  "author": "Yusuke Miyazaki",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --colors",
    "build-all": "npm run build-debug && npm run build-production",
    "build-debug": "npm run build-debug-client && npm run build-debug-server",
    "build-debug-client": "bin/build.sh --debug --client",
    "build-debug-server": "bin/build.sh --debug --server",
    "build-production": "npm run build-production-client && npm run build-production-server",
    "build-production-client": "bin/build.sh --production --client",
    "build-production-server": "bin/build.sh --production --server",
    "start-debug-server": "cp -r build/debug/server/public/ public && node build/debug/server/server.js",
    "start-production-server": "cp -r build/production/server/public/ public && node build/production/server/server.js",
    "supported-browsers": "browserslist \"$(cat browserslist | tr '\n' ',')\"",
    "update-oembed": "bin/oembed.js"
  },
  "license": "LicenseRef-LICENSE",
  "private": true,
  "bugs": {
    "url": "https://github.com/ymyzk/portfolio/issues"
  },
  "homepage": "https://github.com/ymyzk/portfolio",
  "devDependencies": {
    "autoprefixer": "~6.5.0",
    "babel-core": "~6.17.0",
    "babel-eslint": "~7.0.0",
    "babel-loader": "~6.2.0",
    "babel-preset-latest": "~6.16.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-0": "~6.16.0",
    "babel-runtime": "~6.11.6",
    "co": "~4.6.0",
    "compression-webpack-plugin": "~0.3.1",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "~0.25.0",
    "eslint": "~3.7.0",
    "eslint-config-airbnb": "~12.0.0",
    "eslint-import-resolver-webpack": "~0.6.0",
    "eslint-loader": "~1.5.0",
    "eslint-plugin-import": "~1.16.0",
    "eslint-plugin-jsx-a11y": "~2.2.0",
    "eslint-plugin-react": "~6.4.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "html-loader": "~0.4.3",
    "json-loader": "~0.5.4",
    "node-fetch": "~1.6.0",
    "node-sass": "~3.10.0",
    "postcss-loader": "~0.13.0",
    "react-hot-loader": "~1.3.0",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.16.0",
    "yaml-loader": "~0.4.0"
  },
  "dependencies": {
    "babel-polyfill": "~6.16.0",
    "express": "~4.14.0",
    "font-awesome": "~4.6.3",
    "handlebars": "~4.0.5",
    "material-ui": "~0.16.0",
    "moment": "~2.15.0",
    "react": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-helmet": "~3.1.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.8.0",
    "react-tap-event-plugin": "~1.0.0",
    "redux": "~3.6.0",
    "redux-logger": "~2.7.0",
    "ress": "~1.1.1",
    "vary": "~1.1.0",
    "whatwg-fetch": "~1.0.0"
  }
}
