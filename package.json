{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Portfolio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymyzk/portfolio.git"
  },
  "author": "Yusuke Miyazaki",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --colors",
    "build-all": "npm run build-debug && npm run build-production",
    "build-debug": "npm run build-debug-client && npm run build-debug-server",
    "build-debug-client": "webpack --progress",
    "build-debug-server": "webpack --progress --server && mv build/debug/server/public/server.js* build/debug/server/ && cp build/debug/client/bundle.js* build/debug/server/public/",
    "build-production": "npm run build-production-client && npm run build-production-server",
    "build-production-client": "webpack --production --no-color",
    "build-production-server": "webpack --production --no-color --server && mv build/production/server/public/server.js* build/production/server/ && cp build/production/client/bundle.js* build/production/server/public/",
    "start-debug-server": "cp -r build/debug/server/public/ public && node build/debug/server/server.js",
    "start-production-server": "cp -r build/production/server/public/ public && node build/production/server/server.js",
    "supported-browsers": "browserslist \"$(cat browserslist | tr '\n' ',')\""
  },
  "license": "LicenseRef-LICENSE",
  "private": true,
  "bugs": {
    "url": "https://github.com/ymyzk/portfolio/issues"
  },
  "homepage": "https://github.com/ymyzk/portfolio",
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.11.4",
    "babel-eslint": "~6.1.0",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-es2016": "~6.11.0",
    "babel-preset-react": "~6.11.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-runtime": "~6.11.6",
    "compression-webpack-plugin": "~0.3.1",
    "css-loader": "~0.23.1",
    "eslint": "~3.2.2",
    "eslint-config-airbnb": "~10.0.0",
    "eslint-import-resolver-webpack": "~0.4.0",
    "eslint-loader": "~1.5.0",
    "eslint-plugin-import": "~1.12.0",
    "eslint-plugin-jsx-a11y": "~2.0.1",
    "eslint-plugin-react": "~6.0.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "html-autoprefixer": "~0.3.8",
    "html-loader": "~0.4.3",
    "json-loader": "~0.5.4",
    "node-sass": "~3.8.0",
    "postcss-loader": "~0.9.1",
    "react-hot-loader": "~1.3.0",
    "redux-logger": "~2.6.1",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.0",
    "yaml-loader": "~0.4.0"
  },
  "dependencies": {
    "babel-polyfill": "~6.9.1",
    "express": "~4.14.0",
    "font-awesome": "~4.6.3",
    "handlebars": "~4.0.5",
    "material-ui": "0.15.2",
    "moment": "~2.14.0",
    "react": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-helmet": "~3.1.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.6.0",
    "react-tap-event-plugin": "~1.0.0",
    "redux": "~3.5.2",
    "vary": "~1.1.0",
    "whatwg-fetch": "~1.0.0"
  }
}
