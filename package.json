{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Portfolio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymyzk/portfolio.git"
  },
  "author": "Yusuke Miyazaki",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --colors",
    "build-all": "yarn run build-debug && yarn run build-production",
    "build-debug": "yarn run build-debug-client && yarn run build-debug-server",
    "build-debug-client": "bin/build.sh --debug --client",
    "build-debug-server": "bin/build.sh --debug --server",
    "build-production": "yarn run build-production-client && yarn run build-production-server",
    "build-production-client": "bin/build.sh --production --client",
    "build-production-server": "bin/build.sh --production --server",
    "start-debug-server": "cp -r build/debug/server/public/ public && node build/debug/server/server.js",
    "start-production-server": "cp -r build/production/server/public/ public && node build/production/server/server.js",
    "supported-browsers": "browserslist \"$(cat browserslist | tr '\n' ',')\"",
    "update-oembed": "bin/oembed.js"
  },
  "license": "LicenseRef-LICENSE",
  "private": true,
  "bugs": {
    "url": "https://github.com/ymyzk/portfolio/issues"
  },
  "homepage": "https://github.com/ymyzk/portfolio",
  "devDependencies": {
    "babel-core": "^6.22.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "co": "~4.6.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "~4.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-import-resolver-webpack": "~0.8.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.0",
    "html-loader": "~0.4.3",
    "json-loader": "~0.5.4",
    "node-fetch": "^1.7.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^4.0.0",
    "react-hot-loader": "~1.3.0",
    "style-loader": "^0.18.1",
    "url-loader": "~0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "express": "~4.15.0",
    "font-awesome": "~4.7.0",
    "handlebars": "~4.0.5",
    "history": "^4.6.1",
    "immutable": "~3.8.1",
    "material-ui": "^0.18.0",
    "moment": "^2.18.0",
    "prop-types": "~15.5.0",
    "react": "~15.6.0",
    "react-dom": "~15.6.0",
    "react-helmet": "^5.0.0",
    "react-redux": "~5.0.0",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "~2.0.1",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.0",
    "ress": "~1.2.2",
    "smoothscroll": "^0.3.0",
    "vary": "~1.1.0",
    "whatwg-fetch": "~2.0.1"
  }
}
